<div>
    <h1>Hola, {{user.name}} {{user.last_name}} tienes {{user.edad}} Que gusto verte</h1>

    {{#if isAdmin}}
        {{#each food}}
            <div>
                <p>{{this.name}}</p>
                <p>{{this.price}}</p>
            </div>
        {{/each}}
    {{else}}
        <p>No tienes permisos para ver la lista de alimentos</p>
    {{/if}}
</div>

<script src="/js/index.js"></script>